msgstr "`alt+num` — 更改当前控制的机器人ID"

#: ../../source/yaml_config/configuration.md:1022
msgid "`r` — Reset the Environment"
msgstr "`r` — 重置环境"

#: ../../source/yaml_config/configuration.md:1023
msgid "`space` — Toggle Pause/Resume Environment"
msgstr "`space` — 切换暂停/恢复环境"

#: ../../source/yaml_config/configuration.md:1024
msgid "`esc` — Quit the Environment (sets quit flag)"
msgstr "`esc` — 退出环境（设置退出标志）"

#: ../../source/yaml_config/configuration.md:1025
msgid "`x` — Switch Keyboard/Auto Control"
msgstr "`x` — 切换键盘/自动控制"

#: ../../source/yaml_config/configuration.md:1026
msgid "`l` — Reload the Environment"
msgstr "`l` — 重新加载环境"

#: ../../source/yaml_config/configuration.md:1027
