msgid "`w` — Forward"
msgstr "`w` — 前进"

#: ../../source/yaml_config/configuration.md:1014
msgid "`s` — Backward"
msgstr "`s` — 后退"

#: ../../source/yaml_config/configuration.md:1015
msgid "`a` — Turn Left"
msgstr "`a` — 左转"

#: ../../source/yaml_config/configuration.md:1016
msgid "`d` — Turn Right"
msgstr "`d` — 右转"

#: ../../source/yaml_config/configuration.md:1017
msgid "`q` — Decrease Linear Velocity"
msgstr "`q` — 降低线速度"

#: ../../source/yaml_config/configuration.md:1018
msgid "`e` — Increase Linear Velocity"
msgstr "`e` — 提高线速度"

#: ../../source/yaml_config/configuration.md:1019
msgid "`z` — Decrease Angular Velocity"
msgstr "`z` — 降低角速度"

#: ../../source/yaml_config/configuration.md:1020
msgid "`c` — Increase Angular Velocity"
msgstr "`c` — 提高角速度"

#: ../../source/yaml_config/configuration.md:1021
